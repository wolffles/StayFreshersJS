    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "mocha": "6.1.4",
    "nodemon": "^1.18.4",
    "supertest": "4.0.2"
  }
}
